<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_sale_delivery_wizard" model="ir.ui.view">
        <field name="name">Confirm Reveive</field>
        <field name="model">autopos.sale.delivery.wizard</field>
        <field name="arch" type="xml">
            <form string="Confirm">
                <group>
                    <strong>
                        Do you want to delivery
                        <field name="sale_name" readonly="1"/>
                        ?
                    </strong>
                </group>
                <footer>
                    <button string="OK" name="do_confirm_delivery" type="object" class="oe_highlight"/>
                    <button string="Cancel" class="oe_link" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>

