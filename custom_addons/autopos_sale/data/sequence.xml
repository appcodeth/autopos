<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <record model="ir.sequence" id="seq_quotation01">
            <field name="name">Quotation No</field>
            <field name="code">autopos.quotation_no</field>
            <field name="active">TRUE</field>
            <field name="prefix">QT%(y)s%(month)s</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
        <record model="ir.sequence" id="seq_sale01">
            <field name="name">Sale Order No</field>
            <field name="code">autopos.sale_no</field>
            <field name="active">TRUE</field>
            <field name="prefix">SO%(y)s%(month)s</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>
