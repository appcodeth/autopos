<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <record model="ir.sequence" id="seq_rfq01">
            <field name="name">RFQ No</field>
            <field name="code">autopos.rfq_no</field>
            <field name="active">TRUE</field>
            <field name="prefix">RFQ%(y)s%(month)s</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
        <record model="ir.sequence" id="seq_purchase01">
            <field name="name">Purchase Order No</field>
            <field name="code">autopos.purchase_no</field>
            <field name="active">TRUE</field>
            <field name="prefix">PO%(y)s%(month)s</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>

